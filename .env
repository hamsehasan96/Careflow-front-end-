# Application
NODE_ENV=development
PORT=3000
API_VERSION=1.0.0

# Database Configuration
DATABASE_URL=postgresql://postgres:postgres@localhost:5432/careflow
DB_DIALECT=postgres
DB_PROTOCOL=postgres
DB_SSL=false

# Redis Configuration
REDIS_URL=redis://localhost:6379

# Security
CORS_ORIGIN=http://localhost:3000
JWT_SECRET=development_jwt_secret
CSRF_SECRET=development_csrf_secret
WORKER_API_KEY=development_worker_key

# Monitoring
SENTRY_DSN=

# Rate Limiting
RATE_LIMIT_WINDOW=15
RATE_LIMIT_MAX=100

# File Upload Configuration
MAX_FILE_SIZE=10mb

# Database Migration
AUTO_MIGRATE=true
SEED_DATABASE=false

# Logging
LOG_LEVEL=debug 